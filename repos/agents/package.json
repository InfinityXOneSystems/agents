{
  "name": "ai-agent-ecosystem",
  "version": "1.0.0",
  "type": "module",
  "description": "Automated AI Agent Ecosystem with 24/7 maintenance and sync capabilities",
  "main": "dist/index.js",
  "bin": {
    "agents": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "unified": "node dist/unified-orchestrator.js",
    "modules": "node dist/modules/index.js",
    "dev": "ts-node src/index.ts",
    "sync": "node dist/services/auto-sync.js",
    "maintenance": "node dist/agents/maintenance.js",
    "coding": "node dist/agents/coding-agent.js",
    "generate": "node dist/agents/coding-agent.js generate",
    "status": "node dist/agents/coding-agent.js status",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "module-status": "node -e \"require('./dist/modules').getModuleRegistry().getAllStatuses().then(s => console.table(s))\""
  },
  "keywords": [
    "ai",
    "agents",
    "automation",
    "maintenance",
    "sync"
  ],
  "author": "AI Ecosystem",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dockerode": "^4.0.2",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "groq-sdk": "^0.3.0",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "markdown-table": "^3.0.3",
    "node-cron": "^3.0.3",
    "openai": "^4.0.0",
    "pg": "^8.11.0",
    "puppeteer": "^21.0.0",
    "redis": "^4.6.0",
    "simple-git": "^3.20.0",
    "table": "^6.8.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/fs-extra": "^11.0.4",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/markdown-table": "^3.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
